{
  "name": "sway-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build:w",
    "build": "ngc && rm static/app.js*; rollup -c rollup-config.js",
    "build:w": "ngc && rm static/app.js*; rollup -c rollup-config.js --watch",
    "build:p": "ngc && rm static/app.js*; NODE_ENV=production rollup -c rollup-config.js && gzip -9v static/app.js*",
    "postinstall": "echo running npm run build && npm run build",
    "tsc": "tsc",
    "typings": "typings",
    "ngc": "ngc",
    "rollup": "rollup -c rollup-config.js"
  },
  "license": "private",
  "dependencies": {
    "@angular/common": "~2.1.2",
    "@angular/compiler": "~2.1.2",
    "@angular/compiler-cli": "^2.1.2",
    "@angular/core": "~2.1.2",
    "@angular/forms": "~2.1.2",
    "@angular/http": "~2.1.2",
    "@angular/platform-browser": "~2.1.2",
    "@angular/platform-browser-dynamic": "~2.1.2",
    "@angular/platform-server": "^2.1.2",
    "@angular/router": "~3.1.2",
    "@types/core-js": "^0.9.34",
    "@types/node": "~6.0.46",
    "core-js": "~2.4.1",
    "ng2-img-cropper": "swayops/angular2-img-cropper",
    "rollup": "^0.36.3",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0",
    "rxjs": "~5.0.0-rc.1",
    "typescript": "~2.0.6",
    "typings": "~1.5.0",
    "zone.js": "~0.6.26"
  }
}
